{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "plugins": [{ "name": "next" }],
    "module": "es2022",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}
